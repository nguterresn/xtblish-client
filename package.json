{
  "name": "xtblish-client",
  "version": "1.0.20",
  "description": "Compile, sign and send a WASM binary to xtblish",
  "main": "dist/index.js",
  "files": [
    "dist/**"
  ],
  "keywords": [
    "xtblish"
  ],
  "bin": {
    "xtblish": "./dist/index.js"
  },
  "author": "Nuno Nogueira",
  "license": "MIT",
  "scripts": {
    "build": "node --loader ts-node/esm src/index.ts",
    "start": "node ./dist/index.js",
    "dev": "tsc && node ./dist/index.js",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "assemblyscript": "^0.27.35",
    "commander": "^13.1.0",
    "got": "^14.4.6",
    "tracer": "^1.3.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
